<?xml version="1.0"?>
<launch>
    <!-- 底层驱动节点 -->
    <node pkg="jupiter_bringup" type="jupiter_driver.py" name="jupiter_node" required="true" output="screen">
        <param name="imu" value="/jupiter/imu"/>
        <param name="vel" value="/jupiter/get_vel"/>
        <param name="CameraDevice" value="astra"/>
    </node>
    <!-- 校准imu安装时的一些误差 -->
    <node pkg="imu_calib" type="do_calib" name="do_calib" output="screen" required="True">
        <param name="output_file" value="$(find jupiter_bringup)/param/imu/imu_calib.yaml"/>
    </node>
</launch>
